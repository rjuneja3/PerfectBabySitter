@model IEnumerable<JobPosting>

@foreach (JobPosting jb in Model)
{
    <div class="card m-3" style="min-width:18rem; max-width:30.5%; max-height:11rem; min-height:11rem;">
        <div class="carousel-fade" style="height:10rem">
            <h2>@jb.Id</h2>
            <h3>@jb.PhoneNumber</h3>
            <h3>@jb.Language</h3>
            <h3>@jb.Description</h3>
        </div>
    </div>
}

